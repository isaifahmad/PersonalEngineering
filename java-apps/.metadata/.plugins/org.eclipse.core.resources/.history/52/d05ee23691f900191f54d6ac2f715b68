import java.sql.*;

/**
 * 
 */

/**
 * @author saifahmad
 *
 */
public class DBOps {
	
	Connection dbConnection;
	
	DBOps() {
		try {
			Class.forName("sun.jdbc.odbc.JdbcOdbcDriver");
		}
		catch(Exception e) {
			
		}
	}
	
	
	private void getDBConnect() {
		try {
			this.dbConnection = DriverManager.getConnection("jdbc:odbc:localhost", "learner", "learner");
		}
		catch(Exception e) {
			
		}
	}

}
