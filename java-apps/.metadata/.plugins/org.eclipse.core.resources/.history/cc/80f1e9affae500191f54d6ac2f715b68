package workone;

/**
 * @author saifahmad
 *
 */

@WorkAnnotation
public class UseWorkAnnotation {
	
	@WorkAnnotation
	public void fun() {
		
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.out.println("This is an example for using Annotations in Java");

	}

}
